    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "gridfs-stream": "^1.1.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.3.0",
    "migrate": "^1.6.1",
    "moment": "^2.29.4",
    "mongodb": "^3.1.3",
    "mongoose": "^5.2.9",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  }
}
