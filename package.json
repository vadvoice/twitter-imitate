{
  "name": "chat-react",
  "version": "0.0.0",
  "main": "app.js",
  "private": true,
  "scripts": {
    "watch": "NODE_PATH=. nodemon ./bin/www",
    "start:server": "NODE_PATH=. nodemon ./bin/www",
